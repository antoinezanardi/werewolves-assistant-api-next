{
  "name": "werewolves-assistant-api-next",
  "version": "1.35.0",
  "description": "Werewolves Assistant API provides over HTTP requests a way of manage Werewolves games to help the game master.",
  "readme": "README.md",
  "type": "commonjs",
  "keywords": [
    "nest",
    "werewolves",
    "assistant",
    "mongoose",
    "typescript"
  ],
  "homepage": "https://github.com/antoinezanardi/werewolves-assistant-api-next#readme",
  "bugs": {
    "url": "https://github.com/antoinezanardi/werewolves-assistant-api-next/issues",
    "email": "antoine.zanardi@epitech.eu"
  },
  "license": "MIT",
  "author": {
    "name": "Antoine ZANARDI",
    "email": "antoine.zanardi@epitech.eu",
    "url": "https://antoinezanardi.fr"
  },
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoinezanardi/werewolves-assistant-api-next.git"
  },
  "scripts": {
    "prepare": "is-ci || husky",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "docker:dev:start": "docker rmi antoinezanardi/werewolves-assistant-api-dev; docker compose -f docker/werewolves-assistant-api-dev/docker-compose.yml up -d",
    "docker:dev:stop": "docker compose -f docker/werewolves-assistant-api-dev/docker-compose.yml down",
    "docker:dev:reset": "npm run docker:dev:stop && npm run docker:dev:start",
    "docker:production:start": "docker rmi antoinezanardi/werewolves-assistant-api-production; docker compose -f docker/werewolves-assistant-api-production/docker-compose.yml up -d",
    "docker:production:stop": "docker compose -f docker/werewolves-assistant-api-production/docker-compose.yml down",
    "docker:production:reset": "npm run docker:production:stop && npm run docker:production:start",
    "docker:test:start": "docker compose -f docker/werewolves-assistant-api-test/docker-compose.yml up -d",
    "docker:test:stop": "docker compose -f docker/werewolves-assistant-api-test/docker-compose.yml down",
    "docker:test:reset": "npm run docker:test:stop && npm run docker:test:start",
    "start": "npm run start:prod",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged:fix": "lint-staged -c config/lintstaged/.lintstagedrc.eslint.json",
    "lint:inspect-config": "npx @eslint/config-inspector",
    "prettier:gherkin": "prettier --config config/prettier/.prettierrc.cucumber.json --check **/*.feature",
    "prettier:gherkin:fix": "prettier --config config/prettier/.prettierrc.cucumber.json --write **/*.feature",
    "test": "cross-env NODE_ENV=test jest --config config/jest/jest-global.ts",
    "test:staged": "cross-env NODE_ENV=test jest --config config/jest/jest-global.ts -o",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config config/jest/jest-global.ts",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --config config/jest/jest-global.ts",
    "test:stryker": "cross-env NODE_ENV=test rimraf tests/stryker/coverage && stryker run config/stryker/stryker.conf.js",
    "test:stryker:force": "cross-env NODE_ENV=test rimraf tests/stryker/incremental.json tests/stryker/coverage && stryker run config/stryker/stryker.conf.js --force",
    "test:stryker:ci": "cross-env NODE_ENV=test rimraf tests/stryker/coverage && stryker run config/stryker/stryker-ci.conf.js",
    "test:unit": "cross-env NODE_ENV=test jest --config config/jest/jest-unit.ts",
    "test:unit:watch": "cross-env NODE_ENV=test jest --watch --config config/jest/jest-unit.ts",
    "test:unit:cov": "cross-env NODE_ENV=test rimraf tests/unit/coverage && jest --coverage --config config/jest/jest-unit.ts",
    "test:unit:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config tests/unit/jest-unit.ts",
    "test:e2e": "cross-env NODE_ENV=test jest --config config/jest/jest-e2e.ts",
    "test:e2e:watch": "cross-env NODE_ENV=test jest --watch --config config/jest/jest-e2e.ts",
    "test:e2e:cov": "cross-env NODE_ENV=test rimraf tests/e2e/coverage && jest --coverage --config config/jest/jest-e2e.ts",
    "test:e2e:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config config/jest/jest-e2e.ts",
    "test:cucumber": "cross-env NODE_ENV=test cucumber-js --config config/cucumber/cucumber.json; EXIT_CODE_CUCUMBER=$?; npm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:cucumber:publish": "cross-env NODE_ENV=test cucumber-js --config config/cucumber/cucumber.json --publish",
    "test:cucumber:html-report": "tsx tests/acceptance/plugins/html-reporter/cucumber-html-reporter.ts",
    "script:create-branch": "scripts/create-git-branch.sh",
    "script:create-pull-request": "scripts/create-pull-request.sh",
    "gource": "gource --load-config config/gource/gource.config",
    "gcloud:deploy": "gcloud app deploy"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/platform-fastify": "^10.4.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@types/lodash": "^4.17.7",
    "@types/validator": "^13.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dot-object": "^2.1.5",
    "fastify": "^4.28.1",
    "is-ci": "^3.0.1",
    "isobject": "^4.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.5.2",
    "qs": "^6.13.0",
    "radash": "^12.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cucumber/cucumber": "^10.9.0",
    "@eslint/config-inspector": "^0.5.4",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stryker-mutator/core": "^8.5.0",
    "@stryker-mutator/jest-runner": "^8.5.0",
    "@stryker-mutator/typescript-checker": "^8.5.0",
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jest-when": "^3.5.5",
    "@types/node": "^20.16.1",
    "@types/qs": "^6.9.15",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cross-env": "^7.0.3",
    "cucumber-html-reporter": "^7.1.1",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.0",
    "expect": "^29.7.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-when": "^3.6.0",
    "light-my-request": "5.13.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "prettier-plugin-gherkin": "^3.0.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.1.0",
    "semantic-release-export-data": "^1.1.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.17.0",
    "type-fest": "^4.25.0",
    "typescript": "^5.5.4",
    "validate-branch-name": "^1.3.1"
  },
  "engines": {
    "node": ">=21.7.3"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "packageManager": "pnpm@9.7.1"
}