{
  "branches": "main",
  "repositoryUrl": "https://github.com/antoinezanardi/werewolves-assistant-api-next",
  "debug": "false",
  "plugins": [
    ["@semantic-release/commit-analyzer", {
      "preset": "conventionalcommits",
      "releaseRules": [
        {
          "type": "feat",
          "release": "minor"
        },
        {
          "type": "fix",
          "release": "patch"
        },
        {
          "type": "docs",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "perf",
          "release": "patch"
        },
        {
          "type": "test",
          "release": "patch"
        },
        {
          "type": "ci",
          "release": "patch"
        },
        {
          "type": "chore",
          "release": "patch"
        }
      ]
    }],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {
              "type": "feat",
              "section": "üöÄ Features",
              "hidden": false
            },
            {
              "type": "fix",
              "section": "üêõ Bug Fixes",
              "hidden": false
            },
            {
              "type": "docs",
              "section": "üìñ Docs",
              "hidden": false
            },
            {
              "type": "style",
              "section": "üé® Styles",
              "hidden": false
            },
            {
              "type": "refactor",
              "section": "üî© Refactor",
              "hidden": false
            },
            {
              "type": "perf",
              "section": "‚ö°Ô∏è Performances",
              "hidden": false
            },
            {
              "type": "test",
              "section": "‚úÖ Tests",
              "hidden": false
            },
            {
              "type": "ci",
              "section": "üîÅ CI",
              "hidden": false
            },
            {
              "type": "chore",
              "section": "üßπ Chore",
              "hidden": false
            }
          ]
        },
        "writerOpts": {
          "groupBy": "type",
          "commitGroupsSort": ["feat", "fix", "docs", "style", "refactor", "perf", "test", "ci", "chore"]
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md",
        "changelogTitle": "# üê∫ Werewolves Assistant API Versioning Changelog"
      }
    ],
    [
      "@semantic-release/npm",
      {
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md",
          "package.json"
        ]
      }
    ],
    "@semantic-release/github"
  ]
}